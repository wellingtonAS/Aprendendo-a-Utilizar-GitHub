  int main()
 {
    struct simulando
    {
       float preco;
       int parcelas;
    };
    struct simulando agora;
    int y, i, j, u, k, f, l, b, v, q=0, p=0, o=0, r=0, t=0, g=0, h=0, a=0, wa=0;
    char segmento[1];
    char categoria[1];
    char forca[2];
    char sair[1];
    char nome[50];
    char cpf_aux[20];
    int cpf[8];
    int *grupo1;
    int *grupo2;
    int *grupo3;
    int *grupo4;
    int *cota1;
    int *cota2;
    int *cota3;
    int *R;
    int *D;
    grupo1 = &cpf[5];
    grupo2 = &cpf[4];
    grupo3 = &cpf[7];
    grupo4 = &cpf[0];
    cota1 = &cpf[6];
    cota2 = &cpf[3];
    cota3 = &cpf[1];
    R = &cpf[2];
    D = &cpf[6];
do
{
    menusegmento(segmento);
    printf ("DIGITE A OPÇÃO CORRESPONDENTE AO SEGMENTO:\n", setlocale(LC_ALL,""));
    scanf ("%s", segmento);

    while (segmento[0] != '1' && segmento[0] != '3')
    {
      printf ("\a\a\aOpção Inválida !\n", setlocale(LC_ALL, ""));
      scanf("%s", segmento);
    }
    while (q<=2)
    {
         for (i=0;i<=2;i++)
      {
         system ("cls");
         q+=1;
         printf ("\n\n\n\n\n\n\n\n\n\n\n\n\t\t\t\tLoading...");
         system ("cls");
      }
    }
    q=0;

    if (segmento[0] == '1')
    {
        menucategoria(segmento);

        printf ("DIGITE A OPÇÃO CORRESPONDENTE A CATEGORIA:\n", setlocale(LC_ALL,""));
        scanf ("%s", categoria);

        while (categoria[0] != 'B' && categoria[0] != 'b' && categoria[0] != 'M' && categoria[0] != 'm' && categoria[0] != 'A' && categoria[0] != 'a' && categoria[0] !='0')
        {
            printf("\a\a\aErro !\n");
            scanf ("%s", categoria);
        }

        while (p<=2)
        {
           for (j=0;j<=2;j++)
           {
              system ("cls");
              p+=1;
              printf ("\n\n\n\n\n\n\n\n\n\n\t\t\t\tLoading...");
              system ("cls");
           }
        }
        p=0;

    if (categoria[0] == 'B' || categoria[0] == 'b')
    {
      menucategoriabaixa(categoria);

        do
            {
               printf("\n\nINSIRA O PREÇO CORRESPONDENTE A MOTOCICLETA DESEJADA (sem vírgula e sem ponto):\n", setlocale(LC_ALL,""));
               scanf("%f", &agora.preco);

            }while(agora.preco!=5100 && agora.preco!=6006 && agora.preco!=7209 && agora.preco!=7247 && agora.preco!=10299 && agora.preco!=6088 && agora.preco!=8631 && agora.preco!=6852 && agora.preco!=7990 && agora.preco!=9290 && agora.preco!=10720 && agora.preco!=9696 && agora.preco!=12500);

        while (r<=2)
        {
           for (y=0;y<=2;y++)
           {
              system ("cls");
              r+=1;
              printf ("\n\n\n\n\n\n\n\n\n\n\t\t\t\tLoading...");
              system ("cls");
           }
        }
        r=0;

        if (agora.preco==5100)
        {
        menuprecopop(agora.preco);

        printf("\n\nDIGITE A QUANTIDADE DE PARCELAS PARA O PLANO DO SEU CONSÓRCIO:\n", setlocale(LC_ALL,""));

          do
          {
              scanf("%d", &agora.parcelas);
