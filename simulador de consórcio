  char menusegmento(char segmento)
  {
    printf ("\n\n\n\t\t   BEM VINDO AO CONSÓRCIO NACIONAL HONDA\n\n\n", setlocale(LC_ALL,""));
    printf("\n\n");
    printf("|-----+--------+--------+-----+--------+--------+-----+--------+--------------|\n");
    printf("|                                .::SEGMENTOS::.                              |\n");
    printf("|-----+--------+--------+-----+--------+--------+-----+--------+--------------|\n");
    printf("\n\n");
    printf("|-----+--------+--------+-----+--------+--------+-----+--------+--------------|\n");
    printf("|            1           |                         |             3            |\n");
    printf("|-----+--------+--------+-----+--------+--------+-----+--------+--------------|\n");
    printf("|       MOTOCICLETAS     |        ----------       |        MAQUINÁRIO        |\n", setlocale(LC_ALL,""));
    printf("|-----+--------+--------+-----+--------+--------+-----+--------+--------------|\n\n\n");
  }
  char menucategoria(char segmento)
  {
        printf("\n\n\n\n");
        printf("|-----+--------+--------+-----+--------+--------+-----+--------+-----------|\n");
        printf("|                             .::CATEGORIA::.                              |\n");
        printf("|-----+--------+--------+-----+--------+--------+-----+--------+-----------|\n");
        printf("\n\n");
        printf("|-----+--------+--------+-----+--------+--------+-----+--------+-----------|\n");
        printf("|            B          |           M           |             A            |\n");
        printf("|-----+--------+--------+-----+--------+--------+-----+--------+-----------|\n");
        printf("|   BAIXA (cilidrada)   |    MÉDIA (cilidrada)  |     ALTA (cilidrada)     |\n", setlocale(LC_ALL,""));
        printf("|-----+--------+--------+-----+--------+--------+-----+--------+-----------|\n\n\n");
  }
