          wa=0;
        }
    }
   }
}
    else if (segmento[0] == '3')
    {
        printf("\n\n\t\t\tA VENDA DOS PRODUTOS DE FORÇA\n\t\t\tNÃO SÃO INCLUSOS NO SISTEMA DO CONSÓRCIO NACIONAL HONDA\n\t\t\tPOIS A VENDA A PRAZO É REALIZADA\n\t\t\tEXCLUSIVAMENTE NA CONCESSIONÁRIA HONDA\n\t\t\tPORTANTO, OS PRODUTOS AQUI DISPOSTOS\n\t\t\tSERÃO SOMENTE PARA REFERENCIAR SUA COMPRA!\n\n\n");
        system("pause");
        system("cls");
        menusegmentoforca(segmento);

        printf ("DIGITE A OPÇÃO CORRESPONDENTE A UM DOS PRODUTOS DE FORÇA:\n", setlocale(LC_ALL,""));
        scanf ("%s", forca);

        while (forca[1] != '1' && forca[1] != '2' && forca[1] != '3' && forca[1] != '4' && forca [1] != '5')
        {
            printf("\a\a\aErro !\n");
            scanf ("%s", forca);
        }
        while (o<=2)
        {
          for (u=0;u<=2;u++)
          {
            system ("cls");
            o+=1;
            printf ("\n\n\n\n\n\n\n\n\n\n\n\n\t\t\t\tLoading...");
            system ("cls");
          }
        }
        o=0;

        if (forca[1] == '1')
        {
        menusegmentomotobombas(forca);

